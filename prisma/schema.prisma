generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Student {
  id         String     @id
  firstname  String
  lastname   String
  email      String     @unique
  profileUrl String
  profileId  String
  about      String
  level      String
  website    String?
  github     String?
  linkedIn   String?

  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt
}

model Admin {
  id         String      @id @default(uuid())
  email      String      @unique
  password   String      @default("$2a$12$FsMpi7Cp3ukMCg29kQwgge57Wm.HZPaataLPd7AsgLmxeedIpJaLO")
  refreshToken  String?
}